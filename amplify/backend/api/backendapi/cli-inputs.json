{
  "version": 1,
  "serviceConfiguration": {
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "API Key for backend API",
      "expirationTime": 365
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      },
      {
        "mode": "AMAZON_COGNITO_USER_POOLS",
        "cognitoUserPoolId": "auth-kycapp"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      }
    },
    "apiName": "backendapi",
    "gqlSchemaPath": "schema.graphql"
  }
}

